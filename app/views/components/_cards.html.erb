<% @projects.each do |project| %>
    <div class="col-xs-4">
      <div class="card">
        <img src="<%= cl_image_path project.pic_url %>">
        <div class="card-category"><%= project.due_date %></div>
        <div class="card-description">
          <h1><%= link_to project.title, project_path(project.id) %></h1>
          <p><%= project.budget %></p>
        </div>
      </div>
    </div>
  <% end %>

